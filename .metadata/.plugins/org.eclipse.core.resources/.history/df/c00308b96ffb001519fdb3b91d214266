package day1.robot;

import java.awt.Color;
import org.jointheleague.graphical.robot.Robot;

public class MyFirstJavaProgram {
	public static void main(String[] args) {
		
		// START HERE
		Robot r2d4=new Robot();
		r2d4.setPenColor(Color.CYAN);
		r2d4.penDown();
		r2d4.setSpeed();
		for (int i = 0; i <8; i++) {
			r2d4.move(100);
			r2d4.turn(90);
		}
	
		
	}
}
